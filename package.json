{
  "name": "opweb",
  "version": "0.1.0",
  "private": true,
  "homepage": "./",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.35",
    "@fortawesome/free-solid-svg-icons": "^5.15.3",
    "@fortawesome/react-fontawesome": "^0.1.14",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "@types/jest": "^26.0.23",
    "@types/node": "^12.20.15",
    "@types/react": "^17.0.11",
    "@types/react-dom": "^17.0.8",
    "axios": "^0.21.1",
    "bootstrap": "^4.6.0",
    "concurrently": "^6.2.0",
    "cross-env": "^7.0.3",
    "electron-is-dev": "^0.3.0",
    "node-sass": "^6.0.1",
    "node-static": "^0.7.11",
    "react": "^17.0.2",
    "react-bootstrap": "^1.6.1",
    "react-dom": "^17.0.2",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^4.0.3",
    "react-toastify": "^7.0.4",
    "typescript": "^4.3.4",
    "update-electron-app": "^2.0.1",
    "use-sound": "^4.0.1",
    "wait-on": "^6.0.0",
    "web-vitals": "^1.1.2"
  },
  "main": "main.js",
  "scripts": {
    "start": "react-scripts start",
    "build": "webpack --config ./webpack.config.js && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "react-start": "react-scripts start",
    "start-win": "set BROWSER=none && react-scripts start",
    "start-electron": "export ELECTRON_START_URL=http://localhost:3000 && electron .",
    "build-electron": "mkdir build/src && cp -r electron/. build/. && cp -r src/shared/. build/src/shared",
    "build-electron-win": "mkdir build\\src && Xcopy /E /I /Y electron build\\electron",
    "package": "npm run build && npm run build-electron && electron-builder build -c.extraMetadata.main=build/main.js --publish never",
    "package-win": "npm run build && npm run build-electron-win && electron-builder build -c.extraMetadata.main=build/electron/main.js --publish never"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "^7.12.1",
    "@types/react-router-dom": "^5.1.7",
    "electron": "^13.1.5",
    "electron-builder": "^22.11.7",
    "electron-packager": "^15.2.0",
    "electron-webpack": "^2.8.2",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tracerline/OpenProfDesktop.git"
  },
  "build": {
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "publish": {
      "provider": "github",
      "repo": "https://github.com/tracerline/OpenProfDesktop.git",
      "owner": "tracerline38"
    }
  }
}
